#include <stdio.h>

void* memcpy(void* target, const void* source, size_t n)
{
  /* copy an object to another */

  char* targetp = (char*) target;
  const char* sourcep = (const char*) source;

  while (n--)
    *target++ = *sourcep++;

  return target;
}

int main(int argc, char** argv)
{
  

  return 0;
}
