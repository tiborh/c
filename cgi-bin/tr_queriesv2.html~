  <h4>Enter your own "select" query</h4>
  <form action="cgi-bin/cea_sql">
    <p>select<div> 
	<textarea name="message" rows="10" cols="100">
eriref,status,datediff( curdate(), str_to_date(regdate, '%Y-%m-%d')) as 'regdays'
from TR 
where datediff( curdate(), str_to_date(regdate, '%Y-%m-%d') ) <= 3
order by datediff( curdate(), str_to_date(regdate, '%Y-%m-%d') )</textarea>
      </div>
    <div><input type="submit" value="Run Query"></div></p>
  </form>

